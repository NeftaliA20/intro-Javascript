let number = 4936
let ones = number % 10
	 //ones equals six//

number = (number - ones) / 10
	//equals 493//
let tensPlace = number % 10
	//equals three//

number = (number - tensPlace) / 10
	//equal 49//
let hundredthsPlace = number % 10 
	// equal 9//

number = (number - hundredthsPlace) / 10
let thousandthsPlace = number	
