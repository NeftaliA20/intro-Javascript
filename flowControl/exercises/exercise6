// What does this code output to the console?

function isArrayEmpty(arr) {
  if (arr) {
    console.log('Not Empty');
  } else {
    console.log('Empty');
  }
}

isArrayEmpty([]);   // [] is an empty Array

//This code would output 'not empty' because we're still stating
that there is an array by putting the brackets inside the 
paratheses. 